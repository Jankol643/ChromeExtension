{
    "manifest_version": 3,
    "name": "Bookmarks Table Viewer",
    "version": "1.0",
    "description": "Displays all bookmarks in a sortable table with folder filtering.",
    "permissions": [
        "bookmarks"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    }
}